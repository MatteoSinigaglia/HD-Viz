{
  "name": "hdviz",
  "version": "0.8.0",
  "description": "Progetto di Ingegneria del Software 2020/2021, HD-Viz, proposto da Zucchetti S.p.A.",
  "main": "server/server.js",
  "scripts": {
    "start-server": "node server/server.js",
    "dev-server": "nodemon server/server.js",
    "start-client": "cd client && npm start",
    "lint-server": "npx eslint server",
    "lint-client": "npx eslint client",
    "build-client": "cd client && npm build",
    "start": "npm run build-client && npm run start-server",
    "dev": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "test": "cd client && npm test",
    "lint": "npm run lint-server && npm run lint-client"
  },
  "author": "QuaranTeam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuaranTeam2021/HD-Viz.git"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "concurrently": "^6.2.0",
    "core-js": "^3.12.0",
    "eslint": "^7.23.0",
    "nodemon": "^2.0.7",
    "regenerator-runtime": "^0.13.7"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "d3": "^6.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "papaparse": "^5.3.0",
    "pg": "^8.6.0"
  }
}
